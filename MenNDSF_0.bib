keywords = {HOV Alvin (Human Occupied Vehicle)},
month = {aug},
number = {5},
pages = {1127--1137},
title = {{`Pliocardia' krylovata, a new species of vesicomyid clam from cold seeps along the Costa Rica Margin}},
type = {Article},
volume = {92},
year = {2012}
}
