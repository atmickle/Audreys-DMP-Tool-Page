issn = {1664-302X},
journal = {FRONTIERS IN MICROBIOLOGY},
keywords = {HOV Alvin (Human Occupied Vehicle)},
title = {{Denitrification and environmental factors influencing nitrate removal in Guaymas Basin hydrothermally altered sediments}},
type = {Article},
volume = {3},
year = {2012}
}
