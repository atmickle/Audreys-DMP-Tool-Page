keywords = {ROV Jason (Remotely Operated Vehicle)},
month = {nov},
pages = {QOAF03--QOAF03},
publisher = {AMER GEOPHYSICAL UNION},
title = {{Volcanic morphology of West Mata Volcano, NE Lau Basin, based on high-resolution bathymetry and depth changes}},
volume = {12},
