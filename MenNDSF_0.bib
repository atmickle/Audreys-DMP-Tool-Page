@article{Lin2012,
abstract = {The permeable upper oceanic basement serves as a plausible habitat for a variety of microbial communities. There is growing evidence suggesting a substantial subseafloor biosphere. Here new time series data are presented on key inorganic species, methane, hydrogen and dissolved organic carbon (DOC) in ridge flank fluids obtained from subseafloor observatory CORKs (Circulation Obviation Retrofit Kits) at Integrated Ocean Drilling Program (IODP) boreholes 1301A and 1026B. These data show that the new sampling methods (Cowen et al., 2012) employed at 1301A result in lower contamination than earlier studies. Furthermore, sample collection methods permitted most chemical analyses to be performed from aliquots of single large volume samples, thereby allowing more direct comparison of the data. The low phosphate concentrations (0.06–0.2 $\mu$M) suggest that relative to carbon and nitrogen, phosphorus could be a limiting nutrient in the basement biosphere. Coexisting sulfate (17–18 mM), hydrogen sulfide (∼0.1 $\mu$M), hydrogen (0.3–0.7 $\mu$M) and methane (1.5–2 $\mu$M) indicates that the basement aquifer at 1301A either draws fluids from multiple flow paths with different redox histories or is a complex environment that is not thermodynamically controlled and may allow co-occurring metabolic pathways including sulfate reduction and methanogenesis. The low DOC concentrations (11–18 $\mu$M) confirm that ridge flank basement is a net DOC sink and ultimately a net carbon sink. Based on the net amounts of DOC, oxygen, nitrate and sulfate removed (∼30 $\mu$M, ∼80 $\mu$M, ∼40 $\mu$M and ∼10 mM, respectively) from entrained bottom seawater, organic carbon may be aerobically or anaerobically oxidized in biotic and/or abiotic processes.},
annote = {id: 4074},
author = {Lin, H -T and Cowen, J P and Olson, E J and Amend, J P and Lilley, M D},
doi = {10.1016/j.gca.2012.02.017},
issn = {0016-7037},
journal = {Geochimica et Cosmochimica Acta},
keywords = {HOV Alvin (Human Occupied Vehicle),ROV Jason (Remotely Operated Vehicle)},
pages = {213--227},
title = {{Inorganic chemistry, gas compositions and dissolved organic carbon in fluids from sedimented young basaltic crust on the Juan de Fuca Ridge flanks}},
url = {http://dx.doi.org/10.1016/j.gca.2012.02.017},
volume = {85},
year = {2012}
}
